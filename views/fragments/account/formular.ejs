<form
  id="form_inreg"
  method="post"
  class="date"
  action="<%= locals.action %>"
  enctype="multipart/form-data">
  <p>
    <label for="inp-username">
      Username:
    </label>
    <input
      type="text"
      name="username"
      id="inp-username"
      value="prof<%= Math.floor(Math.random()*100000) %>"
      required pattern="[A-Za-z0-9]+">
  </p>

  <p>
    <label for="inp-nume">
      Nume:
    </label>
    <input
      type="text"
      name="nume"
      id="inp-nume"
      value="Gogulescu">
  </p>

  <p>
    <label for="inp-prenume">
      Prenume:
    </label>
    <input
      type="text"
      name="prenume"
      id="inp-prenume"
      value="Gogu"
      pattern="[A-Z][a-z]+([-\.][A-Z][a-z]+)?">
  </p>

  <p>
    <label for="parl">
      Parola:
    </label>
    <input
      id="parl"
      type="password"
      required
      name="parola"
      value="">
  </p>

  <p id="p-reintrod">
    <label for="rparl">
      Reintroduceti parola:
    </label>
    <input
      id="rparl"
      required
      type="password"
      name="rparola"
      value="">
  </p>

  <p>
    <label for="inp-email">
      Email:
    </label>
    <input
      type="text"
      name="email"
      id="inp-email"
      required
      value="profprofprof007@gmail.com"
      size="50">
  </p>

  <p>
    <label for="sel-culoare">
      Culoare text chat:
    </label>
    <select name="culoareText" id="sel-culoare">
      <option value="red" selected>rosu</option>
      <option value="blue">albastru</option>
      <option value="green">verde</option>
    </select>
  </p>

  <p>
    <label for="inp-poza">
      Poza:
    </label>
    <input
      id="inp-poza"
      type="file"
      name="poza">
  </p>

  <p>
    <input
      id="inpt-submt"
      type="submit"
      value="Trimite">
    <input
      id="inp-reset"
      type="reset"
      value="Reseteaza">
  </p>
</form>